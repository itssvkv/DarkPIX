<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/base_nav"    app:startDestination="@id/splashFragment">    <fragment        android:id="@+id/splashFragment"        android:name="com.itssvkv.darkpix.ui.splash.SplashFragment"        android:label="fragment_splash"        tools:layout="@layout/fragment_splash" >        <action            android:id="@+id/splashToOnBoarding"            app:destination="@id/onBoardingFragment"            app:enterAnim="@android:anim/fade_in"            app:exitAnim="@android:anim/fade_out"            app:popUpTo="@id/splashFragment"            app:popUpToInclusive="true" />        <action            android:id="@+id/splashFragmentToAuthFragment"            app:destination="@id/authFragment"            app:enterAnim="@android:anim/fade_in"            app:exitAnim="@android:anim/fade_out"            app:popUpTo="@id/splashFragment"            app:popUpToInclusive="true" />        <action            android:id="@+id/splashFragmentToBottomNavFragment"            app:destination="@id/btmNavFragment2"            app:enterAnim="@android:anim/fade_in"            app:exitAnim="@android:anim/fade_out"            app:popUpTo="@id/splashFragment"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/onBoardingFragment"        android:name="com.itssvkv.darkpix.ui.onboarding.OnBoardingFragment"        android:label="fragment_on_boarding"        tools:layout="@layout/fragment_on_boarding" >        <action            android:id="@+id/onBoardingFragmentToAuthFragment"            app:destination="@id/authFragment"            app:enterAnim="@android:anim/fade_in"            app:exitAnim="@android:anim/fade_out"            app:popUpTo="@id/onBoardingFragment"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/authFragment"        android:name="com.itssvkv.darkpix.ui.auth.AuthFragment"        android:label="fragment_auth"        tools:layout="@layout/fragment_auth" >        <action            android:id="@+id/authFragmentToBottomNav"            app:destination="@id/btmNavFragment2"            app:enterAnim="@android:anim/fade_in"            app:exitAnim="@android:anim/fade_out"            app:popUpTo="@id/authFragment"            app:popUpToInclusive="true" />    </fragment>    <include app:graph="@navigation/bottom_bar_nav_graph" />    <fragment        android:id="@+id/btmNavFragment2"        android:name="com.itssvkv.darkpix.ui.bottomnav.TabsFragment"        android:label="fragment_tabs"        tools:layout="@layout/fragment_tabs" >        <action            android:id="@+id/btmNavFragmentToNavGraph"            app:destination="@id/bottom_bar_nav_graph" />        <action            android:id="@+id/btmNavFragmentToOnePhotoFragment"            app:destination="@id/onePhotoFragment2"            app:enterAnim="@android:anim/fade_in"            app:exitAnim="@android:anim/fade_out" />    </fragment>    <fragment        android:id="@+id/onePhotoFragment2"        android:name="com.itssvkv.darkpix.ui.home.clickedphoto.OnePhotoFragment"        android:label="fragment_one_photo"        tools:layout="@layout/fragment_one_photo" >        <action            android:id="@+id/onePhotoFragmentToBtmNavFragment"            app:destination="@id/btmNavFragment2"            app:enterAnim="@android:anim/fade_in"            app:exitAnim="@android:anim/fade_out"            app:popUpTo="@id/onePhotoFragment2"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/searchPhotosFragment"        android:name="com.itssvkv.darkpix.ui.search.searchphotos.SearchPhotosFragment"        android:label="fragment_search_photos"        tools:layout="@layout/fragment_search_photos" >        <action            android:id="@+id/searchPhotosToOnePhoto"            app:destination="@id/onePhotoFragment2" />        <action            android:id="@+id/searchPhotoToSearchFragment"            app:destination="@id/btmNavFragment2"            app:popUpTo="@id/searchPhotosFragment"            app:popUpToInclusive="true" />    </fragment></navigation>